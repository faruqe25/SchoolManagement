@model List<SchoolManagement.Areas.Admin.ViewModels.ExamTypeVm>

    <p class="h2" style="text-align:center;">Exam Type List<br /><div style="border-bottom:    1px solid #686868"></div> </p>

    <table class="table table-striped table-responsive">
        <thead>
            <tr>
                <th class="h3">
                    Serial
                </th>
                <th class=" h3 ">
                    Class Name
                </th>
                <th class=" h3  ">
                    Action
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td class=" h4  ">@item.Serial</td>
                    <td class=" h4  ">@item.exmnameVM</td>
                    <td class=" h4  "><a asp-action="UpdateExam" asp-controller="Home" asp-area="Admin" asp-route-id="@item.exmidVM"><i class="fa fa-pencil-square" style="font-size:25px;color:green"></i></a></td>
                </tr>
            }
        </tbody>
    </table>
